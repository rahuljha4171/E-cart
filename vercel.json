{
  "version": 2,
  "builds": [
    {
      "src": "./server/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "./client/build",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server/app.js"
    }
  ]
}
